<Window x:Class="ControlPanel.View.DeleteSubscriptionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlPanel.View"
        mc:Ignorable="d"
        Title="Удаление абонемента" Height="129" Width="368">
    <Grid>
        <Label x:Name="LabelDeleteSubscription" Content="Выберите абонемент который хотите удалить:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="14"/>
        <ComboBox x:Name="CbSubscription" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top" Width="154"/>
        <Button x:Name="ButtonDeleteSubscription" Content="удалить абонемент" HorizontalAlignment="Left" Margin="193,44,0,0" VerticalAlignment="Top" Width="117" Height="22" Click="ButtonDeleteSubscription_Click"/>

    </Grid>
</Window>

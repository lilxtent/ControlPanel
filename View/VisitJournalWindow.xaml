<Window x:Class="ControlPanel.View.VisitJournalWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlPanel.View"
        mc:Ignorable="d"
        Title="VisitJournalWindow" Height="485" Width="200">
    <Grid Margin="0,0,10,18">
        <DataGrid x:Name="VisitJournalTable" AutoGenerateColumns="False" IsReadOnly="True" HorizontalAlignment="Left" Height="416" VerticalAlignment="Top" Width="160" Margin="10,10,0,0">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Дата посещения" Binding="{Binding Path=Date}" Width="150"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
